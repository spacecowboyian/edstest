<% const title = "Sidebar"; %>

<title><%= title %></title>

<section class="example-page">

  <%- include("_header.ejs", {
    classes: ["example-page_#header"],
    title,
    stability: 2,
    summary: "The sidebar contains navigation items and links the user will use to navigate between sections within the application."
  }) _%>

  <div class="example-page_#content">

<%
  ["basic", "blue", "raspberry", "gray"].forEach((color, i) => {
%>
    <div class="<%= color === "gray" ? "bg-white" : "bg-gray-100" %> flex flex-row items-center h-256<%= i ? " mt-12" : "" %>">
      <div class="eds-sidebar eds-sidebar.open eds-sidebar.<%= color %>">
        <div class="eds-sidebar_#content">
<% [0, 1, 2].forEach(sectionIndex => { %>
          <div class="eds-sidebar_.section">
            <h1 class="eds-sidebar_.eds-menu-header eds-sidebar_.when-open eds-menu-header eds-menu-header.<%= color %>">
              Header <%= sectionIndex + 1 %>
            </h1>
<% [0, 1, 2].forEach(menuItemIndex => { %>
            <a href="#" class="eds-sidebar_.eds-menu-item eds-sidebar.open_.eds-menu-item eds-menu-item eds-menu-item.<%= color %><%= sectionIndex === 0 && menuItemIndex === 1 ? "-selected" : "" %>">
              <svg class="eds-sidebar_.eds-menu-item_#icon eds-sidebar.open_.eds-menu-item_#icon eds-menu-item_#icon" viewBox="0 0 24 24">
                <path d="M3 13h8V3H3v10zm0 8h8v-6H3v6zm10 0h8V11h-8v10zm0-18v6h8V3h-8z"/>
              </svg>
              <span class="eds-menu-item_#label eds-sidebar_.when-open">Dashboard</span>
            </a>
<% }) %>
          </div>
<% }) %>
        </div>
        <div class="eds-sidebar_#controls">
          <a
            href="#"
            class="eds-sidebar_.eds-sidebar-toggle eds-sidebar.open_.eds-sidebar-toggle eds-sidebar-toggle eds-sidebar-toggle.<%= color %>"
            onclick="['eds-sidebar.open', 'eds-sidebar.closed'].forEach(function(c) { this.closest('.eds-sidebar').classList.toggle(c); }.bind(this));['eds-sidebar.open_.eds-menu-item', 'eds-sidebar.closed_.eds-menu-item'].forEach(function(c) { [].forEach.call(this.closest('.eds-sidebar').querySelectorAll('.eds-sidebar_\\.eds-menu-item'), function(e) { e.classList.toggle(c); }) }.bind(this));['eds-sidebar.open_.eds-sidebar-toggle','eds-sidebar.closed_.eds-sidebar-toggle'].forEach(function (c) { this.classList.toggle(c); }.bind(this));['eds-sidebar.open_.eds-menu-item_#icon', 'eds-sidebar.closed_.eds-menu-item_#icon'].forEach(function(c) { [].forEach.call(this.closest('.eds-sidebar').querySelectorAll('.eds-sidebar_\\.eds-menu-item_\\#icon'), function(e) { e.classList.toggle(c); }) }.bind(this))"
            >
            <svg class="eds-sidebar-toggle_#icon eds-sidebar_.when-open" viewBox="0 0 24 24">
              <path d="M18.41 16.59L13.82 12l4.59-4.59L17 6l-6 6 6 6zM6 6h2v12H6z"/>
            </svg>
            <svg class="eds-sidebar-toggle_#icon eds-sidebar_.when-closed" viewBox="0 0 24 24">
              <path d="M5.59 7.41L10.18 12l-4.59 4.59L7 18l6-6-6-6zM16 6h2v12h-2z"/>
            </svg>
            <span class="eds-sidebar-toggle_#label eds-sidebar_.when-open">Collapse Menu</span>
          </a>
        </div>
      </div>
      <div class="flex-1 text-center font-sans font-400 text-black text-16 antialiased">Content area</div>
    </div>
<% }) %>

    <div class="bg-gray-100 flex flex-row items-center h-256 mt-12">
      <div class="eds-sidebar eds-sidebar.open eds-sidebar.blue">
        <div class="eds-sidebar_#content">
          <div class="eds-sidebar_.section">
            <a
              href="#"
              class="eds-sidebar_.eds-menu-item eds-sidebar.open_.eds-menu-item eds-menu-item eds-menu-item.blue eds-menu-item.open"
              onclick="['eds-menu-item.open', 'eds-menu-item.closed'].forEach(function(c) { this.classList.toggle(c); }.bind(this)); (function(nes) { var h = nes.classList.toggle('eds-menu-item.hidden'); [nes.nextElementSibling, nes.nextElementSibling.nextElementSibling].forEach(function(e) { e.classList[h ? 'add' : 'remove']('eds-menu-item.hidden') }); })(this.nextElementSibling)"
              >
              <svg class="eds-sidebar_.eds-menu-item_#icon eds-sidebar.open_.eds-menu-item_#icon eds-menu-item_#icon" viewBox="0 0 24 24">
                <path d="M19.43 12.98c.04-.32.07-.64.07-.98s-.03-.66-.07-.98l2.11-1.65c.19-.15.24-.42.12-.64l-2-3.46c-.12-.22-.39-.3-.61-.22l-2.49 1c-.52-.4-1.08-.73-1.69-.98l-.38-2.65C14.46 2.18 14.25 2 14 2h-4c-.25 0-.46.18-.49.42l-.38 2.65c-.61.25-1.17.59-1.69.98l-2.49-1c-.23-.09-.49 0-.61.22l-2 3.46c-.13.22-.07.49.12.64l2.11 1.65c-.04.32-.07.65-.07.98s.03.66.07.98l-2.11 1.65c-.19.15-.24.42-.12.64l2 3.46c.12.22.39.3.61.22l2.49-1c.52.4 1.08.73 1.69.98l.38 2.65c.03.24.24.42.49.42h4c.25 0 .46-.18.49-.42l.38-2.65c.61-.25 1.17-.59 1.69-.98l2.49 1c.23.09.49 0 .61-.22l2-3.46c.12-.22.07-.49-.12-.64l-2.11-1.65zM12 15.5c-1.93 0-3.5-1.57-3.5-3.5s1.57-3.5 3.5-3.5 3.5 1.57 3.5 3.5-1.57 3.5-3.5 3.5z"/>
              </svg>
              <span class="eds-menu-item_#label eds-sidebar_.when-open">Settings</span>
            </a>
            <a
              href="#"
              class="eds-sidebar_.eds-menu-item eds-sidebar.open_.eds-menu-item eds-menu-item eds-menu-item.blue eds-menu-item.indent eds-menu-item.open"
              onclick="['eds-menu-item.open', 'eds-menu-item.closed'].forEach(function(c) { this.classList.toggle(c); }.bind(this));[this.nextElementSibling, this.nextElementSibling.nextElementSibling].forEach(function(e) { e.classList.toggle('eds-menu-item.hidden') })"
              >
              <svg class="eds-sidebar_.eds-menu-item_#icon eds-sidebar.open_.eds-menu-item_#icon eds-menu-item_#icon">
                <path d="M12 1L3 5v6c0 5.55 3.84 10.74 9 12 5.16-1.26 9-6.45 9-12V5l-9-4zm0 10.99h7c-.53 4.12-3.28 7.79-7 8.94V12H5V6.3l7-3.11v8.8z"/>
              </svg>
              <span class="eds-menu-item_#label eds-sidebar_.when-open">Security</span>
            </a>
            <a href="#" class="eds-sidebar_.eds-menu-item eds-sidebar.open_.eds-menu-item eds-menu-item eds-menu-item.blue eds-menu-item.indent2">
              <svg class="eds-sidebar_.eds-menu-item_#icon eds-sidebar.open_.eds-menu-item_#icon eds-menu-item_#icon">
                <path d="M20 5H4c-1.1 0-1.99.9-1.99 2L2 17c0 1.1.9 2 2 2h16c1.1 0 2-.9 2-2V7c0-1.1-.9-2-2-2zm-9 3h2v2h-2V8zm0 3h2v2h-2v-2zM8 8h2v2H8V8zm0 3h2v2H8v-2zm-1 2H5v-2h2v2zm0-3H5V8h2v2zm9 7H8v-2h8v2zm0-4h-2v-2h2v2zm0-3h-2V8h2v2zm3 3h-2v-2h2v2zm0-3h-2V8h2v2z"/>
              </svg>
              <span class="eds-menu-item_#label eds-sidebar_.when-open">Password</span>
            </a>
            <a href="#" class="eds-sidebar_.eds-menu-item eds-sidebar.open_.eds-menu-item eds-menu-item eds-menu-item.blue eds-menu-item.indent2">
              <svg class="eds-sidebar_.eds-menu-item_#icon eds-sidebar.open_.eds-menu-item_#icon eds-menu-item_#icon">
                <path d="M4 6h18V4H4c-1.1 0-2 .9-2 2v11H0v3h14v-3H4V6zm19 2h-6c-.55 0-1 .45-1 1v10c0 .55.45 1 1 1h6c.55 0 1-.45 1-1V9c0-.55-.45-1-1-1zm-1 9h-4v-7h4v7z"/>
              </svg>
              <span class="eds-menu-item_#label eds-sidebar_.when-open"><span style="white-space: nowrap">Two-Factor</span><br/>Authentication</span>
            </a>
          </div>
        </div>
        <div class="eds-sidebar_#controls">
          <a
            href="#"
            class="eds-sidebar_.eds-sidebar-toggle eds-sidebar-toggle eds-sidebar-toggle.blue"
            onclick="['eds-sidebar.open', 'eds-sidebar.closed'].forEach(function(c) { this.closest('.eds-sidebar').classList.toggle(c); }.bind(this));['eds-sidebar.open_.eds-menu-item', 'eds-sidebar.closed_.eds-menu-item'].forEach(function(c) { [].forEach.call(this.closest('.eds-sidebar').querySelectorAll('.eds-sidebar_\\.eds-menu-item'), function(e) { e.classList.toggle(c); }) }.bind(this));['eds-sidebar.open_.eds-sidebar-toggle','eds-sidebar.closed_.eds-sidebar-toggle'].forEach(function (c) { this.classList.toggle(c); }.bind(this));['eds-sidebar.open_.eds-menu-item_#icon', 'eds-sidebar.closed_.eds-menu-item_#icon'].forEach(function(c) { [].forEach.call(this.closest('.eds-sidebar').querySelectorAll('.eds-sidebar_\\.eds-menu-item_\\#icon'), function(e) { e.classList.toggle(c); }) }.bind(this))"
            >
            <svg class="eds-sidebar-toggle_#icon eds-sidebar_.when-open" viewBox="0 0 24 24">
              <path d="M18.41 16.59L13.82 12l4.59-4.59L17 6l-6 6 6 6zM6 6h2v12H6z"/>
            </svg>
            <svg class="eds-sidebar-toggle_#icon eds-sidebar_.when-closed" viewBox="0 0 24 24">
              <path d="M5.59 7.41L10.18 12l-4.59 4.59L7 18l6-6-6-6zM16 6h2v12h-2z"/>
            </svg>
            <span class="eds-sidebar-toggle_#label eds-sidebar_.when-open">Collapse Menu</span>
          </a>
        </div>
      </div>
      <div class="flex-1 text-center font-sans font-400 text-black text-16 antialiased">Content area</div>
    </div>

  </div>

</section>
