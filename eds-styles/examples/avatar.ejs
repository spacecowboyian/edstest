<% const title = "Avatar"; %>

<title><%= title %></title>

<section class="example-page">

  <%- include("_header.ejs", {
    classes: ["example-page_#header"],
    title,
    stability: 2,
    summary: "Avatar is an object that represents the user's identity in the application (e.g., the user's picture, their initials, or an icon)."
  }) _%>

  <div class="example-page_#content">

    <div class="inline-flex items-center p-12">
      <div class="eds-avatar eds-avatar.blue eds-avatar.sm mr-12">
        <!-- transparent placeholder image for accessibility -->
        <img
          src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
          alt="Unknown User" />
      </div>
      <div class="eds-avatar eds-avatar.blue eds-avatar.md mr-12" data-initials="AL">
        <!-- transparent placeholder image for accessibility -->
        <img
          src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
          alt="Alan Lindsay" />
      </div>
      <div class="eds-avatar eds-avatar.blue eds-avatar.lg">
        <img
          src="data:image/jpeg;base64,"
          alt="Alan Lindsay"
          style="width:100%" />
      </div>
    </div>

    <div class="inline-flex items-center p-12 bg-gray-100">
      <div class="eds-avatar eds-avatar.gray eds-avatar.sm mr-12">
        <!-- transparent placeholder image for accessibility -->
        <img
          src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
          alt="Unknown User" />
      </div>
      <div class="eds-avatar eds-avatar.gray eds-avatar.md mr-12" data-initials="AL">
        <!-- transparent placeholder image for accessibility -->
        <img
          src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
          alt="Alan Lindsay" />
      </div>
      <div class="eds-avatar eds-avatar.gray eds-avatar.lg">
        <img
          src="data:image/jpeg;base64,"
          alt="Alan Lindsay"
          style="width:100%" />
      </div>
    </div>

    <div class="inline-flex items-center p-12 bg-purple-700">
      <div class="eds-avatar eds-avatar.inverse eds-avatar.sm mr-12">
        <!-- transparent placeholder image for accessibility -->
        <img
          src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
          alt="Unknown User" />
      </div>
      <div class="eds-avatar eds-avatar.inverse eds-avatar.md mr-12" data-initials="AL">
        <!-- transparent placeholder image for accessibility -->
        <img
          src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
          alt="Alan Lindsay" />
      </div>
      <div class="eds-avatar eds-avatar.inverse eds-avatar.lg">
        <img
          src="data:image/jpeg;base64,"
          alt="Alan Lindsay"
          style="width:100%" />
      </div>
    </div>

  </div>

</section>
